{
  "manifest_version": 2,
  "name": "Caitlyn's first extension",
  "description": "First attempt at browser extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file://*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
  "128": "images/icon.png"
  }
}
